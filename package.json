{
  "name": "realtydatalabs-backend",
  "version": "1.0.0",
  "description": "Backend API for RealtyDataLabs",
  "main": "backend/server.js",
  "scripts": {
  "start": "node backend/server.js",
  "dev:server": "node backend/server.js",
